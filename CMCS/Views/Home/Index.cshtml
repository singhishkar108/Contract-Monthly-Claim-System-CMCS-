@{
    ViewData["Title"] = "Home Page";
}

<div id="carExampleControls" class="carousel slide" data-bs-ride="carousel" data-bs-interval="3000">
    <div class="carousel-inner">
        <!-- image 1 -->
        <div class="carousel-item active">
            <img class="d-block w-100" src="~/assets/cmcs image1.png" alt="First slide" />
        </div>

        <!-- image 2 -->
        <div class="carousel-item">
            <img class="d-block w-100" src="~/assets/cmcs image2.png" alt="Second slide" />
        </div>

        <!-- image 3 -->
        <div class="carousel-item">
            <img class="d-block w-100" src="~/assets/cmcs image3.png" alt="Third slide" />
        </div>

        <!-- image 4 -->
        <div class="carousel-item">
            <img class="d-block w-100" src="~/assets/cmcs image 4.png" alt="Fourth slide" />
        </div>

        <!-- image 5 -->
        <div class="carousel-item">
            <img class="d-block w-100" src="~/assets/cmcs image5.png" alt="Fifth slide" />
        </div>
    </div>

    <!-- previous slide control -->
    <a class="carousel-control-prev" href="#carExampleControls" role="button" data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="sr-only"></span>
    </a>

    <!-- next slide control -->
    <a class="carousel-control-next" href="#carExampleControls" role="button" data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="sr-only"></span>
    </a>
</div>

